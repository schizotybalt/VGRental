@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-large">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-4">
        <h2>View all customers</h2>
        <p>@Html.ActionLink("Customers »", "index", "customer", routeValues: null, htmlAttributes: new { @class = "btn btn-default" })</p>
        @*<p><a class="btn btn-default" href="~/customer">Customers &raquo;</a></p>*@
    </div>
    <div class="col-md-4">
        <h2>View all subscriptions</h2>
        <p>@Html.ActionLink("Subscriptions »", "index", "subscription", routeValues: null, htmlAttributes: new { @class = "btn btn-default" })</p>
        @*<p><a class="btn btn-default" href="~/subscription">Subscriptions &raquo;</a></p>*@
    </div>
    <div class="col-md-4">
        <h2>Create a subscription</h2>
        <p>@Html.ActionLink("Subscribe »", "create", "subscription", routeValues: null, htmlAttributes: new { @class = "btn btn-default" })</p>
        @*<p><a class="btn btn-default" href="~/subscription/create">Subscribe &raquo;</a></p>*@
    </div>
</div>
<div class="row">
    <div class="col-md-4">
        <h2>View all games</h2>
        <p>@Html.ActionLink("Games »", "index", "game", routeValues: null, htmlAttributes: new { @class = "btn btn-default" })</p>
    </div>
    <div class="col-md-4">
        <h2>View all orders</h2>
        <p>@Html.ActionLink("Orders »", "index", "order", routeValues: null, htmlAttributes: new { @class = "btn btn-default" })</p>
    </div>
</div>